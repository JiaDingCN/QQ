<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jiading.controller.SignInPanelController">
   <children>
      <Label layoutX="105.0" layoutY="119.0" text="用户名" />
      <Label layoutX="105.0" layoutY="163.0" text="密码" />
      <TextField fx:id="usernameField" layoutX="185.0" layoutY="115.0" />
      <PasswordField fx:id="passwordField" layoutX="185.0" layoutY="159.0" />
      <Button fx:id="submitButton" layoutX="102.0" layoutY="245.0" mnemonicParsing="false" onMouseClicked="#submit" text="登录" />
      <Button fx:id="exitButton" layoutX="300.0" layoutY="245.0" mnemonicParsing="false" onMouseClicked="#exit" text="退出" />
   </children>
</AnchorPane>
