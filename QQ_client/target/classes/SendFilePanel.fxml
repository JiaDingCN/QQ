      <Label fx:id="pathLabel" layoutX="158.0" layoutY="116.0" text="path" />
      <Button fx:id="chooseFile" layoutX="393.0" layoutY="112.0" mnemonicParsing="false" onMouseClicked="#chooseFile" text="选择" />
      <Button fx:id="sendButton" layoutX="121.0" layoutY="267.0" mnemonicParsing="false" onMouseClicked="#send" text="发送" />
